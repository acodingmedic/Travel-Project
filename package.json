{
  "name": "travelai-platform",
  "version": "1.0.0",
  "description": "Intelligent travel planning platform powered by AI with swarm intelligence capabilities",
  "main": "src/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node src/index.js",
    "start:frontend": "craco start",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "craco build",
    "build:backend": "node scripts/build-backend.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest src/**/*.test.js --testPathIgnorePatterns=src/frontend",
    "test:frontend": "craco test --watchAll=false",
    "test:watch": "concurrently \"npm run test:backend -- --watch\" \"npm run test:frontend -- --watchAll\"",
    "lint": "eslint src --ext .js,.jsx --fix",
    "lint:check": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/index.js",
    "dev:frontend": "craco start",
    "docker:build": "docker build -t travelai-platform .",
    "docker:run": "docker run -p 3000:3000 -p 8000:8000 travelai-platform",
    "deploy:staging": "npm run build && npm run deploy:staging:backend && npm run deploy:staging:frontend",
    "deploy:production": "npm run build && npm run deploy:production:backend && npm run deploy:production:frontend",
    "deploy:staging:backend": "echo 'Deploy backend to staging'",
    "deploy:staging:frontend": "echo 'Deploy frontend to staging'",
    "deploy:production:backend": "echo 'Deploy backend to production'",
    "deploy:production:frontend": "echo 'Deploy frontend to production'",
    "health-check": "node scripts/health-check.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "clean": "rimraf build dist node_modules/.cache",
    "analyze": "npm run build:frontend && npx bundle-analyzer build/static/js/*.js",
    "security-audit": "npm audit && npm run lint:security",
    "lint:security": "eslint src --ext .js,.jsx --config .eslintrc.security.js",
    "precommit": "npm run lint:check && npm run format:check && npm run test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "moment": "^2.29.4",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "redis": "^4.6.10",
    "sharp": "^0.33.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/node": "^20.10.4",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "path-browserify": "^1.0.1",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/frontend/**",
      "!src/**/*.test.{js,jsx}",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "keywords": [
    "travel",
    "ai",
    "artificial-intelligence",
    "swarm-intelligence",
    "trip-planning",
    "vacation",
    "booking",
    "travel-assistant",
    "intelligent-search",
    "recommendation-system",
    "nodejs",
    "react",
    "express",
    "mongodb",
    "redis"
  ],
  "author": {
    "name": "TravelAI Team",
    "email": "team@travelai.com",
    "url": "https://travelai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/travelai/platform.git"
  },
  "bugs": {
    "url": "https://github.com/travelai/platform/issues"
  },
  "homepage": "https://travelai.com",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/travelai"
  }
}
